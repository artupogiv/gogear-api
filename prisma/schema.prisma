generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id String @id @default(ulid())

  slug String  @unique
  name String
  desc String?

  imageUrl String

  price Decimal

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
